				<timecontrol>
					<enable>
						<processtag>guestBathBlindTimeEnable</processtag>
					</enable>
					<events>
						<event hour="00" minute="01" action="openAtSunrise">
							<dateranges>
								<daterange>
									<start day="1" month="1"></start>
									<end day="31" month="12"></end>
