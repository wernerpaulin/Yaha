				</input>
			</inputs>
			
			<outputs>
				<output name="scaledvalue">
					<processtag>actTempBurner</processtag>
				</output>
			</outputs>
		</sensor>
		<sensor id="tempFlowRadiator" linearizationcurveid="burner">
